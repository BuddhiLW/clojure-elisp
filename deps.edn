{:paths ["src" "resources"]
 :deps {org.clojure/clojure {:mvn/version "1.12.0"}
        org.clojure/tools.analyzer {:mvn/version "1.2.0"}
        org.clojure/tools.reader {:mvn/version "1.5.2"}
        ;; hive-dsl â€” Result ADT for railway-oriented error handling
        io.github.hive-agi/hive-dsl
        {:git/tag "v0.3.1" :git/sha "0e28484"}}
 :aliases
 {:dev {:extra-paths ["dev" "test"]
        :extra-deps {nrepl/nrepl {:mvn/version "1.3.0"}
                     cider/cider-nrepl {:mvn/version "0.58.0"}
                     org.clojure/test.check {:mvn/version "1.1.1"}
                     io.github.hive-agi/hive-test
                     {:git/tag "v0.1.2" :git/sha "8894fa1"}}}
  :test {:extra-paths ["test"]
         :extra-deps {lambdaisland/kaocha {:mvn/version "1.91.1392"}
                      org.clojure/test.check {:mvn/version "1.1.1"}
                      io.github.hive-agi/hive-test
                      {:git/tag "v0.1.2" :git/sha "8894fa1"}}
         :main-opts ["-m" "kaocha.runner"]}
  :build {:deps {io.github.clojure/tools.build {:mvn/version "0.10.9"}}
          :ns-default build}

